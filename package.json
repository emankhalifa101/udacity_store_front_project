  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.17",
    "@types/pg": "^8.6.6",
