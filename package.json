  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "npx tsc",
    "start:prod": "npm run build && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix",
    "format": "npx prettier --write src/**/*.ts ",
    "jasmine": "jasmine-ts  --config=./spec/support/jasmine.json",
    "test": "tsc &&export NODE_ENV=test&& db-migrate up --config ./database.json -e test&&tsc && npx jasmine && db-migrate reset",
    "test:reset": "tsc &&export NODE_ENV=test&& db-migrate reset",
    "migration:up": "npx db-migrate up"
  },
